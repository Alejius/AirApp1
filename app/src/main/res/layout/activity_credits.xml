<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#6C8EAA">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="40dp"
        android:layout_height="44dp"
        app:srcCompat="@drawable/barras"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="5dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="113dp"
        android:layout_height="40dp"
        android:height="50px"
        android:fontFamily="@font/lufga"
        android:hint="50px"
        android:text="Mi hogar"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="236dp"
        android:layout_height="66dp"
        android:fontFamily="@font/lufga"
        android:text="Smart AC"
        android:textSize="34sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="423dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="337dp"
        android:layout_height="147dp"
        android:fontFamily="@font/lufga"
        android:text="Smart AC es una aplicación creada por y para personas que hacen uso a menudo de sus aires acondicionados."
        android:textSize="16sp"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="489dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="338dp"
        android:layout_height="98dp"
        android:fontFamily="@font/lufga"
        android:text="Ésta aplicación fue desarrollada por Agustina Fornés, Alejo Perna, Melanie Rodríguez, Francesco Santone y Julián Swerdlin"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="591dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="444dp"
        android:layout_height="417dp"
        app:srcCompat="@drawable/codeteam"
        tools:layout_editor_absoluteX="-17dp"
        tools:layout_editor_absoluteY="33dp" />
</androidx.constraintlayout.widget.ConstraintLayout>